<urlrewrite>
<rule>
        <from>/(.*).htm</from>  
        <to>/$1.jsp</to> 
</rule>

<rule>
        <from>/总表.xls</from>  
        <to>/notfound</to> 
</rule>
<rule>
        <from>/user.txt</from>  
        <to>/notfound</to> 
</rule>
</urlrewrite>